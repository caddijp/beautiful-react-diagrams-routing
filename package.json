  "eslintConfig": {
    "extends": [
      "@caddijp/eslint-config",
      "prettier/react"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx,json,yml,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(scss|css|less|svg)$": "identity-obj-proxy"
    }
