{
  "name": "beautiful-react-diagrams-routing",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "repository": "ssh://git@github.com/caddijp/beautiful-react-diagrams-routing.git",
  "author": "Keisuke Kan <keisuke_kan@caddi.jp>",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p . --outDir lib",
    "build-storybook": "build-storybook",
    "lint": "run-p -l lint:*",
    "lint:es": "eslint . --ext=ts,tsx",
    "lint:prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "peerDependencies": {
    "beautiful-react-diagrams": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@caddijp/eslint-config": "0.2.1",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/addon-storyshots": "6.2.9",
    "@storybook/react": "6.2.9",
    "@types/dagre": "0.7.44",
    "@types/jest": "26.0.23",
    "@types/rosie": "0.0.39",
    "babel-loader": "8.2.2",
    "beautiful-react-diagrams": "0.3.1",
    "conventional-changelog-cli": "2.1.1",
    "dagre": "0.8.5",
    "eslint": "7.25.0",
    "husky": "6.0.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "prettier": "2.2.1",
    "prettier-plugin-organize-imports": "1.1.1",
    "prettier-plugin-package": "1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "17.0.2",
    "react-test-renderer": "17.0.2",
    "rosie": "2.1.0",
    "ts-jest": "26.5.6",
    "typescript": "4.2.4"
  },
  "types": "lib/index.d.ts",
  "eslintConfig": {
    "extends": [
      "@caddijp/eslint-config",
      "prettier/react"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(scss|css|less|svg)$": "identity-obj-proxy"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx,json,yml,md}": [
      "prettier --write"
    ]
  }
}
